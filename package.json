{
  "name": "appstack-react",
  "version": "1.2.2",
  "description": "AppStack - ReactJS Admin & Dashboard Template",
  "author": "Paul Laros <support@bootlab.io>",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-brands-svg-icons": "5.8.2",
    "@fortawesome/free-regular-svg-icons": "5.8.2",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "animejs": "^3.1.0",
    "apexcharts": "^3.8.6",
    "availity-reactstrap-validation": "2.5.3",
    "axios": "^0.19.0",
    "bootstrap": "4.3.1",
    "chokidar-cli": "1.2.2",
    "classnames": "2.2.6",
    "fullcalendar": "3.10.0",
    "google-map-react": "1.1.4",
    "i18next": "^17.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "jquery": "^3.4.1",
    "jvectormap-next": "3.0.0",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.2",
    "react": "16.8.6",
    "react-apexcharts": "1.3.3",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-bootstrap-table-next": "3.1.3",
